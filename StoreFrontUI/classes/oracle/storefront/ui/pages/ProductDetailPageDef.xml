<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.10.8" id="ProductDetailPageDef"
                Package="oracle.storefront.ui.pages">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="BrowseProducts" RangeSize="25" DataControl="FODProductAMDataControl" id="BrowseProductsIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="BrowseProductsIterator" id="ProductName">
      <AttrNames>
        <Item Value="ProductName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="BrowseProductsIterator" id="Description">
      <AttrNames>
        <Item Value="Description"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="BrowseProductsIterator" id="CategoryName">
      <AttrNames>
        <Item Value="CategoryName"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
