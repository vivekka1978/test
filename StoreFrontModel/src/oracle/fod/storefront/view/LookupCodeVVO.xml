  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="oracle.fod.storefront.view.LookupCodeVVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindLookupType"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT  
    LOOKUP_CODES.LOOKUP_TYPE LOOKUP_TYPE,  
    LOOKUP_CODES.LOOKUP_CODE LOOKUP_CODE,  
    LOOKUP_CODES.MEANING MEANING,  
    LOOKUP_CODES.DESCRIPTION DESCRIPTION,  
    LOOKUP_CODES.LANGUAGE LANGUAGE  
FROM  
    LOOKUP_CODES  
WHERE  
    LOOKUP_CODES.LANGUAGE = SYS_CONTEXT('USERENV', 'LANG')  
    AND LOOKUP_CODES.LOOKUP_TYPE =:BindLookupType]]></SQLQuery>
  <ViewAttribute
